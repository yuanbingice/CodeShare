# Uncomment this line to define a global platform for your project
# platform :ios, '9.0'

target 'CodeShare' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  #添加约束的头文件  高于0的版本，写这个限制和什么都不写是一个效果，都表示使用最新版本
  pod "SnapKit", "~>  0.0"

  #分类的集合,可以快速设置按钮的背景颜色,添加事件 /////不显式指定依赖库版本，表示每次都获取最新版本
  pod "JKCategories"
  
  #使用大于等于3.0但小于4.0 // 相当于3.x版本
  pod "Alamofire", "~> 3.0"
  
  #Mob产品公共库
  pod "MOBFoundation_IDFA"
  
  #短信发送 SDK
  pod "SMSSDK"
  
  #这个是用于iOS上的函数响应式编程框架,如果想要在iOS上使用MVVM模式,配合这个框架最合适
  #ReactiveCocoa
  pod "ReactiveCocoa", "~> 4.0"

  # 处理项目中所有的键盘问题,导入库文件即可,不用再添加代码
  pod "IQKeyboardManager", "~> 3.0"
  
  # 全屏侧滑返回, 也不需要再添加代码
  pod "FDFullscreenPopGesture"

  #处理model,图片网络加载等等
  pod "YYModel"

  # Pods for CodeShare

end
